{
    "manifest_version": 2,
    "name": "ClickSaver",
    "version": "1.0",
    "description": "Just open image page and click save!",
    "icons": {
        "128": "icon128.png",
        "48": "icon48.png",
        "16": "icon16.png"
    },
    "background": {
        "page": "eventPage.html"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["main.js"]
    }],
    "permissions": [
        "tabs",
        "background",
        // "contextMenus",
        "downloads"
    ]
}